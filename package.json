{
  "name": "code-tracking-extension",
  "version": "0.0.1",
  "description": "VS Code extension for code tracking",
  "main": "dist/extension.js",
  "repository": {"url": "https://github.com/jagadeeshm2002/code-tracking-extension"},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./dist/test/runTest.js",
    "lint": "eslint src -c ts",
    "prepare": "npm run compile"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "codeTrackingView",
          "name": "Code Tracking"
        }
      ]
    },
    "commands": [
      {
        "command": "codeTracking.connectGitHub",
        "title": "Code Tracking: Connect GitHub"
      },
      {
        "command": "codeTracking.toggleTracking",
        "title": "Code Tracking: Toggle Tracking"
      }
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.68",
    "@types/vscode": "^1.85.0 || ^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.1",
    "axios": "^1.6.2",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "mocha": "^11.0.1",
    "node-fetch": "^3.3.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
